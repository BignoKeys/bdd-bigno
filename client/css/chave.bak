/* CSS reset */
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, caption {
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	font-size: 100%;
	vertical-align: baseline;
	background: transparent;
}

/* Fundo, links e caixa central */
body {
	background: url(21.jpg) no-repeat center top;
	background-color: #FFAD60;
	font-family: Georgia, serif;
	font-size: 14px;
	line-height: 21px;
}

/* Barra de rolagem */
::-webkit-scrollbar {
    width: 10px;
}
 
/* Fundo */
::-webkit-scrollbar-track {
	background-color: #F27224;
}
 
/* Indicador */
::-webkit-scrollbar-thumb {
	background-color: rgba(81,10,84,0.4);
}
::-webkit-scrollbar-thumb:window-inactive {
	background-color: rgba(81,10,84,0.2); 
}

/* Links */
a:link, a:visited {
	text-decoration: none;
	color: #470054;
}

div.contem {
	width: 1000px;
	margin: 0 auto;
}

/* Header */

header {
	margin: 0;
	padding: 0;
	border: 0;
	overflow: auto;
}

/* Menu */
nav ul {
	list-style-type: none;
	margin: 0;
	padding: 0 50px;
	float: right;
}

nav li {
	float: left;
	padding: 0 5px;
}

nav li a {
	display: block;
	background-color: #FFAD60;
	color: #470054;
	text-align: center;
	padding: 14px 16px;
	text-decoration: none;
	font-weight: bold;
	font-family: Helvetica, Arial, sans-serif;
}

nav li a:hover {
	background-color: #F27224;
}

/* Logotipo */
div.logo {
	display: block;
	float: left;
	margin: 50px 0 44px 50px;
}

/* Fundo translucido artigo*/
div.qbranco {
	overflow: auto;
	background-color: white;
	padding: 40px 50px;
}

/* Título */
div.qbranco h1 {
	font-size: 21px;
	line-height: 21px;
	font-weight: normal;
	margin: 5px 0 16px 13px;
	color: #F27224;
	float: left;
}

div.qbranco h2 {
	font-size: 18px;
	line-height: 21px;
	font-weight: normal;
	color: #F27224;
	margin: 13px 13px;
}

/* Div esquerda */
div.cesquerda {
	width: 437px;
	margin: 0 13px 0 0;
	float: left;
}

/* Div direita */
div.cdireita {
	width: 437px;
	margin: 0 0 0 13px;
	float: left;
}


/* Busca */
.formbusca {
	float:right;
	width: 225px;
	padding-right: 50px;
	text-align: right;
}

.fbcampo {
	margin: 0;
	padding: 5px 10px;
	font-family: Georgia, serif;
	font-size: 14px;
	color: #F27224;
	border:1px solid #FFAD60;
	border-right:0px;
}

.fbbotao {
	margin: 0;
	padding: 5px 7px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 13px;
	font-weight:bold;
	outline: none;
	cursor: pointer;
	text-align: center;
	text-decoration: none;
	color: #F27224;
	border: solid 1px #FFAD60;
	background: #FFAD60;
}

.fbbotao:hover {
	text-decoration: none;
	background: #F27224;
}

.fbbotao::-moz-focus-inner {
  border: 0;
}

.fbclear{
	clear:both;
}

/* Quadrados conteúdo */
div.roxinho {
	background-color: #FDF0E9;
	width: 412px;
	height: 387px;
	padding: 25px 0 25px 25px;
	overflow: auto;
	position: relative;
}

/* Botões expandir/ocultar */
.expocu {
	margin: 0;
	padding: 5px 7px;
	font-size: 13px;
	font-weight:bold;
	outline: none;
	cursor: pointer;
	text-align: center;
	text-decoration: none;
	color: #470054;
	border: solid 1px #FFAD60;
	background: #FFAD60;
}

.expocu:hover {
	background-color: #F27224;
}

.btnident {
	margin: 0;
	padding: 5px 7px;
	font-size: 13px;
	font-weight:bold;
	outline: none;
	cursor: pointer;
	text-align: center;
	text-decoration: none;
	border: solid 1px #F27224;
	background: #F27224;
	color: white;
}

.btnident:hover {
	color: #470054;
}

div.divaeh {
	width: 375px;
	text-align: right;
	margin: 0 37px 7px 0;
	display: block;
	clear: both;
}

div.agregadordescritores {
	width: 356px;
}

ul.descritor {
	padding: 0 0 7px 19px !important;
	width: 356px;
}

div.valoresn {
	padding: 0 0 7px 19px !important;
	width: 337px;
	font-size: 14px;
}

form.valornum {
	margin: 0;
}

img.lala {
	padding: 0 6px 0 0;
}

/* Textos descritores*/
div.agregadordescritores p {
	font-size: 16px;
	line-height: 21px;
}

/* Campos descritores numéricos */
.numnum {
	margin: 0;
	padding: 5px 10px;
	font-family: Georgia, serif;
	font-size: 14px;
	color: #F27224;
	border:1px solid #FFAD60;
}

/* Imagens descritores */
div.valoresi {
	padding: 0 0 0 10px !important;
	width: 333px;
	margin: 0;
	float: none;
	overflow: auto;
}

div.vimagens {
	float: left;
	width: 100px;
	margin: 2px 0 5px 10px;
	position: relative;
	left: 0;
	top: 0;
	cursor: pointer;
}

div.vimagens.selecionado {
	background-color: #FFAD60;
	-moz-box-shadow: 0px 0px 0px 2px #F27224;
	-webkit-box-shadow: 0px 0px 0px 2px #F27224;
	box-shadow: 0px 0px 0px 2px #F27224;
}

img.vimg {
	width: 100px;
	height: 100px;
	position: relative;
	top: 0;
	left: 0;
}

img.vglos {
	width: 14px;
	height: 14px;
	position: absolute;
	top: 3px;
	left: 3px;
}

div.vimagens p {
	font-size: 14px;
	line-height: 16px;
	text-align: center;
}

/* Toggle Chaves */
ul.accordion {
	list-style: none;
	padding: 0;
}

ul .inner {
	list-style: none;
	overflow: hidden;
	display: none;
}

ul .inner.show {
	/*display: block;*/
}

ul li {
	margin: 0;
}

ul li a.toggle {
	display: block;
	font-size: 16px;
	line-height: 21px;
	font-weight: normal;
	padding: 0 0 3px 0;
}

ul li a.toggle span {
	display: inline-block;
	vertical-align: 1px;
	width: 8px;
	height: 8px;
	padding: 2px;
	border-style: solid;
	border-width: 2px;
	border-color: #A978B3;
	color: #A978B3;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	line-height: 10px;
	text-align: center;
}

/* Descritores Selecionados*/
.descsel {
	width: width: 356px;
}

.descsel input[type=checkbox] {
	display:none;
}

.descsel label {
	font-size: 16px;
	line-height: 21px;
	font-weight: normal;
	color: #F27224;
}

.descsel input[type=checkbox] + label:before {
	content: "\00a0";  
	display: inline-block;
	vertical-align: 1px;
	width: 8px;
	height: 8px;
	padding: 2px;
	border-style: solid;
	border-width: 2px;
	border-color: #A978B3;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #A978B3;
	line-height: 10px;
	text-align: center;
	margin: 0 5px 0 0;
}

.descsel input[type=checkbox]:checked + label:before {
	content: "\00d7";
    display: inline-block;
	vertical-align: 1px;
	width: 8px;
	height: 8px;
	padding: 2px;
	border-style: solid;
	border-width: 2px;
	border-color: #A978B3;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #A978B3;
	line-height: 10px;
	text-align: center;
	margin: 0 5px 0 0;
}

/* Espécies */
div.especies {
	margin: 0 0 12px 0;
	padding: 12px;
	width: 351px;
	background-color: #F9C6A7;
	overflow: auto;
	display: flex;
	align-itens: center;
}

div.especies img {
    min-height: 100px;
    min-width: 100px;
	width: 100px;
	height: 100px;
	float: left;
	margin: 0 12px 0 0;
	order: <integer>;
	text-align: center;
}

div.nsp {
	order: <integer>;
	margin: auto 0;
}

p.nomesp {
	font-size: 18px;
	line-height: 21px;
	font-weight: normal;
}

p.famisp {
	font-weight: normal;
	line-height: 21px;
	font-size: 16px;
	margin: 7px 0 0 0;
}

/* Rodapé */
footer {
	display: block;
	background-color: #F27224;
	padding: 25px 50px;
	text-align: center;
	margin: 0 0 50px 0;
	color: white;
	font-size: 12px;
}